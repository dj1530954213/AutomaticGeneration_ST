程序名称:AO_CONVERT
子程序变量:AO_VARIABLE
{{~ for point in points ~}}
(* AO点位: {{ point.hmi_tag_name }} - {{ point.description }} *)
ENGIN_HEX_{{ point.hmi_tag_name }}(
    AV:={{ point.hmi_tag_name }}, 
    MU:={{ point.range_high ?? 100.0 }},
    MD:={{ point.range_low ?? 0.0 }},
    WU:=65535,
    WD:=0,
    WH=>{{ get_address point }}
);

{{~ end ~}}